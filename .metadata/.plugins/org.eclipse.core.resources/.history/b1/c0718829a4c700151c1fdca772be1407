import unittest
from language import *
import unittest

class CLangTestCase(unittest.TestCase):
    def setUp(self):
        unittest.TestCase.setUp(self)
        clang = CLan()
        
    def testfilter(self):
        name1 = "xx.c"
        self.failUnless(clang.filter(name1))
        
print __name__
if __name__ == '__main__': unittest.main()